        });

      
        //2016.9.26
        //-----轮播
    var curIndex = 0,imgLen = $(".CharContent7_imgwra img").length; 
        function changeTo(num){
          
            if (window.innerWidth< 1170) {
                var goLeft = num * 785;
            } else {
                var goLeft = num * 830;
            }
            $(".CharContent7_imgwra").animate({ left: "-" + goLeft + "px" }, 500);
            $(".CharContent7_bgimg").find("li").removeClass("active").eq(num).addClass("active"); 
            $(".CharContent7_bgimg").find("li").removeClass("active").eq(num).addClass("active");
        }
        var autoChange = setInterval(function(){
            if(curIndex < imgLen-1){
                curIndex ++;
            }else{
                curIndex = 0;
            }
            changeTo(curIndex);
        },2500);
        //左箭头滑入滑出事件处理
        $("#CharContent7_left").hover(function () {
            //滑入清除定时器
            clearInterval(autoChange);
        },function(){
            //滑出则重置定时器
            autoChangeAgain();
        });
        //左箭头点击处理
        $("#CharContent7_left").click(function () {
            //根据curIndex进行上一个图片处理
            curIndex = (curIndex > 0) ? (--curIndex) : (imgLen - 1);
            changeTo(curIndex);
        });
        //右箭头滑入滑出事件处理
        $("#CharContent7_right").hover(function () {
            //滑入清除定时器
            clearInterval(autoChange);
        },function(){
            //滑出则重置定时器
            autoChangeAgain();
        });
        //右箭头点击处理
        $("#CharContent7_right").click(function () {
            curIndex = (curIndex < imgLen - 1) ? (++curIndex) : 0;
            changeTo(curIndex);
        });
        //对右下角按钮index进行事件绑定处理等
        $(".CharContent7_bgimg").find("li").each(function (item) { 
            $(this).hover(function(){
                clearInterval(autoChange);
                changeTo(item);
                curIndex = item;
            },function(){
                autoChangeAgain();
            });
        });
        //清除定时器时候的重置定时器--封装
        function autoChangeAgain(){
            autoChange = setInterval(function(){
                if(curIndex < imgLen-1){
                    curIndex ++;
                }else{
                    curIndex = 0;
                }
                //调用变换处理函数
                changeTo(curIndex);
            },2500);
        }
        //-----轮播

