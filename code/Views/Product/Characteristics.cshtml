                    "z-index": "1"
                })
                $(".content8_CharImg" + i).prev().css({ "left": "40px", "width": "320px", "z-index": "0", "top": "67px" })
                $(".content8_CharImg" + i).next().css({ "left": "390px", "z-index": "0", "width": "320px", "top": "67px" })
            })
       
