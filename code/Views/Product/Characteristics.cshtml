        //导航
        var scroll = $(window.document).scrollTop();
        if (scroll > 190) {
            $(".top_banner_bg").addClass("change");
        } else {
            $(".top_banner_bg").removeClass("change");
        }
        $(window).bind("scroll", function () {
            var scroll = $(window.document).scrollTop();
            if (scroll > 360) {
                $(".CharWrapper .bannerNav").addClass("change");
                $("#getPrize_banner").hide();
                $("#menu_div").css({ "top": "0px", "display": "none" });

            } else {
                $(".CharWrapper .bannerNav").removeClass("change");
                $("#getPrize_banner").show();
                $("#menu_div").css({ "top": "0px", "display": "block" })
            }

            if ($(".CharWrapper .bannerNav").hasClass("change")) {
                $(".top_banner_bg  .lkrep_item").hover(function () {
                    $(window.document).scrollTop(320)
                })
            } else {
                $(".top_banner_bg  .lkrep_item").hover(function () {
                    $(window.document).scrollTop(0)
                })
            }
            //导航下划线
            if (scroll < 1440 && scroll > 460) {
                $(".bannerNav_wra li").eq(0).addClass("active").siblings().removeClass("active");
            } else if (scroll > 1440 && scroll < 2190) {
                $(".bannerNav_wra li").eq(1).addClass("active").siblings().removeClass("active");
            } else if (scroll > 2190 && scroll < 2970) {
                $(".bannerNav_wra li").eq(2).addClass("active").siblings().removeClass("active");
            } else if (scroll > 2970 && scroll < 4060) {
                $(".bannerNav_wra li").eq(3).addClass("active").siblings().removeClass("active");
            } else if (scroll > 4060 && scroll < 4840) {
                $(".bannerNav_wra li").eq(4).addClass("active").siblings().removeClass("active");
            } else if (scroll > 4840 && scroll < 5560) {
                $(".bannerNav_wra li").eq(5).addClass("active").siblings().removeClass("active");
            } else if (scroll > 5560 && scroll < 6455) {
                $(".bannerNav_wra li").eq(6).addClass("active").siblings().removeClass("active");
            } else if (scroll > 6455 && scroll < 7303) {
