                                        <div class="price-type c_5">微商城增强版+经销商管理</div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                    <div class="price_div productBg01">
                        <div class="produce_border">
                            <div class="border01"></div>
                            <div class="border02 " style="display:none;"></div>
                            <div class="border04" style="display:none;"></div>
                            <div class="border05" style="display:none;"></div>
                            <div class="border06" style="display:none;"></div>
                        </div>
                        <div class="userNum">
                            <div class="user-left">请选择用户数</div>
                            <ul id="ul_users" data-bind="foreach:selectedProduct().scaleList" style="width:auto !important">
                                <li class="li-user" data-bind="css:{'user-selected':$parent.selectedScaleId() == priceid},event:{click:doChangeScale}">
                                    <span class="span_usercount" data-bind="text:scale"></span><span data-bind="text:$parent.selectedProduct().scale_unit"></span>
                                </li>
                            </ul>
                            <input type="hidden" id="select_base_product_flag" data-bind="value:selectedProduct().product_flag" />
                            <input type="hidden" class="selectedProductId" data-bind="value:selectedProductId()" />
                            <input type="hidden" class="selectedScaleId" data-bind="value:selectedScaleId()" />
                            <input type="hidden" class="selectedYearId" data-bind="value:selectedYearId()" />
                            <input type="hidden" class="selectedprice" data-bind="value:selectedYear().price" />
                            <input type="hidden" class="selectedoldprice" data-bind="value:selectedYear().oldprice" />
                            <input type="hidden" id="qq_href" value="@(qq)" />
                            <input type="hidden" id="companyid" value="@(companyid)" />
                            @Html.Hidden("discount_two_year", discount_two_year)
                            @Html.Hidden("discount_three_year", discount_three_year)
                            <a class="more-user" target="_blank" href="@(qq)">更多用户</a>
                        </div>
                        <div class="userNum">
                            <div class="user-left">请选择使用年限</div>
                            <ul id="years">
                                <li id="1" onclick="doChangeYear(this)" class="li-year user-selected">1年</li>
                                <li id="2" onclick="doChangeYear(this)" class="li-year">2年</li>
                                <li id="3" onclick="doChangeYear(this)" class="li-year">3年</li>
                            </ul>
                            <a class="more-user" target="_blank" href="@(qq)">更多年限</a>
                        </div>
                        <div class="userNum" style="width:254px;margin-right:0;">
                            <div class="user-left" style="text-align:left;">价格</div>
                            @*<div class="oldPrice" style="height:28px;"> <div class="original-price" id="div_old_money" style="display:none;">原价：<span id="oldTotalMoney">￥8940</span>元/操作员 · 年</div></div>*@
                            <div class="oldPrice" style="height:28px;"> <div class="original-price" id="div_old_money" >单价：<span id="unitPrice">￥420</span>元/操作员 · 年</div></div>
                            <div class="money" style="width: 100%; text-align: left;">总价：<span>￥<span id="totalMoney">420</span>.00</span></div>

                            <div class="buy-Btn">
                                @if (CC.Framework.User.Current == null && string.IsNullOrEmpty(Request["companyid"]))
                                {
                                    <a id="btnBuyBase" href="/buy/CompanySure?type=0">联系客服</a>
                                }
                                else
                                {
                                    <a id="btnBuyBase" onclick="submitOrder()">联系客服</a>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
