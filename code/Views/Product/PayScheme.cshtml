@using CC.Public;
@using CC.InnerBLL.Enum;
@{

    ViewBag.Title = "进销存价格|免费进销存软件哪个好|进销存管理软件|来肯云商";
    ViewBag.Keywords = "进销存软件价格,进销存软件多少钱";
    ViewBag.description = "进销存价格多少钱?来肯云商提供进销存软件免费版,进销存软件精简版/标准版价格,每天不足2元钱;以及增强版。包含进销存,财务管理,客户管理,移动办公等11大功能!";

    Layout = "~/Views/Shared/_Layout.cshtml";

    string cdnFileRoot = CC.ERP.Portal.Global.CDNFileRoot;
    ViewData.Add("cdnFileRoot", cdnFileRoot);

    HashObject servicer = Model == null ? null : (HashObject)Model["servicer"];
    string qq = servicer == null ? CC.InnerBLL.Tools.LoadInnerCfg("official_qq") : servicer.GetString("qq");
    qq = string.Format("http://wpa.qq.com/msgrd?v=3&uin={0}&site=qq&menu=yes", qq);
    decimal discount_two_year = 1, discount_three_year = 1;
    decimal.TryParse(CC.InnerBLL.Tools.LoadInnerCfg("discount_two_year"), out discount_two_year);
    decimal.TryParse(CC.InnerBLL.Tools.LoadInnerCfg("discount_three_year"), out discount_three_year);

    long companyid = CC.Framework.User.Current != null ? CC.Framework.User.Current.CurrentProfileid : 0;
    if (companyid == 0 && !string.IsNullOrEmpty(Request["companyid"]))
    {
        long.TryParse(Request["companyid"], out companyid);
    }

}
<script src="@(cdnFileRoot)/Scripts/knockout-3.1.0.js"></script>
<script src="@(cdnFileRoot)/Scripts/4.0/buy.js?v=20170808"></script>
<div class="pricePage">
    <div class="priceBanner">
        <div class="price-top">
            <div class="priceBanner_wrapper">
                <div class="price-edition" id="div_product" data-bind="foreach:list">
                    <ul id="ul_products" data-bind="foreach:productList" class="price_ul">
                        <li data-bind="attr:{productid:productid,productflag:product_flag},event:{hover:doChangeSelectProduct},css:{'li-border':$parent.selectedProductId() == productid || ($parent.selectedProductId() == null && productid == @((int)LkVersion.Simplify))}">
                            <div data-bind="if:product_flag == @((int)LkVersion.Simplify)">
                                <div class="li-div">
                                    <div>
                                        <div class="price-title c_1">@(ShowEnumName.GetEnumName<LkVersion>((int)LkVersion.Simplify))</div>
                                        <div class="edition edition-img-1"></div>
                                        <div class="price-type c_1">单店或单仓库且人员不多</div>
                                    </div>
                                </div>
                            </div>
                            <div data-bind="if:product_flag == @((int)LkVersion.Standard)">
                                <div class="li-div">
                                    <div>
                                        <div class="price-title c_2">@(ShowEnumName.GetEnumName<LkVersion>((int)LkVersion.Standard))</div>
                                        <div class="edition edition-img-2"></div>
                                        <div class="price-type c_2">多店或多仓库分权管理</div>
                                    </div>
                                </div>
                            </div>
                            <div data-bind="if:product_flag == @((int)LkVersion.Integration_Standard)">
                                <div class="li-div">
                                    <div>
                                        <div class="price-title c_3">@(ShowEnumName.GetEnumName<LkVersion>((int)LkVersion.Integration_Standard))</div>
                                        <div class="edition edition-img-3"></div>
                                        <div class="price-type c_3">进销存标准版+专属微商城<br />线上线下一体化管理（O2O）</div>
                                    </div>
                                </div>
                            </div>
                            <div data-bind="if:product_flag == @((int)LkVersion.Integration_Strengthen)">
                                <div class="li-div">
                                    <div>
                                        <div class="price-title c_4">@(ShowEnumName.GetEnumName<LkVersion>((int)LkVersion.Integration_Strengthen))</div>
                                        <div class="edition edition-img-4"></div>
                                        <div class="price-type c_4">一体化标准版+多门店独立<br />微商城+社交化分销</div>
                                    </div>
                                </div>
                            </div>
                            <div data-bind="if:product_flag == @((int)LkVersion.Linkage)">
                                <div class="li-div">
                                    <div>
                                        <div class="price-title c_5">@(ShowEnumName.GetEnumName<LkVersion>((int)LkVersion.Linkage))</div>
