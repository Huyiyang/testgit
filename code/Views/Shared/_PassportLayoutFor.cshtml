<script type="text/javascript">
    function showService() {
        $("#service_close").animate({
            marginLeft: 0,
            opacity: 0.65,
            height: 0
        }, 300);
        $("#service_open").animate({
            marginTop: 0,
            marginLeft: -170,
            opacity: 1
        }, 300);
    }

    function closeService() {
        $("#service_open").animate({
            marginLeft: 0,
            opacity: 1,
            marginTop: -48
        }, 300);
        $("#service_close").animate({
            marginLeft: -54,
            opacity: 0.65,
            height: 48
        }, 300);
    }

    function LoadBaiduScript() {
        var src = "https://hm.baidu.com/hm.js?3fb0899d09b1e084b9e74c9717ca4ef9";
        var nodeScript = document.createElement("script");
        nodeScript.setAttribute("type", "text/javascript");
        nodeScript.setAttribute("src", src);

        $("#div_script")[0].appendChild(nodeScript);
    }

    $(function () {

        setTimeout(LoadBaiduScript, 500);
        //检测到手机设备自动跳转
        var mobileAgent = new Array("iphone", "android", "mobile", "blackberry", "webos", "incognito", "webmate", "bada", "nokia", "lg", "ucweb", "skyfire");

        var browser = navigator.userAgent.toLowerCase();

        var isMobile = false;
        for (var i = 0; i < mobileAgent.length; i++) {
            if (browser.indexOf(mobileAgent[i]) != -1 && !navigator.userAgent.match(/iPod/i) && !navigator.userAgent.match(/Pad/i)) {
                isMobile = true;
                location.href = 'http://m.lk361.com?agentid=@(AgentInfo.Current.AgentID)&salerid=@(Request["salerid"])&sourceflag=@(Request["sourceflag"])';
                break;
            }
        }

        $(".lazy").lazyload();
        $("img").attr("alt", "在线进销存,进销存软件,经销商管理,crm管理系统,免费进销存软件,进销存软件哪个好,进销存软件免费版");

        closeService();

        $("#service_top").click(function () {
            closeService();
        });

        $("#service_close").click(function () {
            showService();
        });

        //计算内容离顶部的距离
        var height = $(window).height();
        var register_div_height = $(".register_div").height();
        $(".register_div").each(function () {
            if ($(this).height() > register_div_height) {
                register_div_height = $(this).height();
            }
        });
        var padding_top = (height - register_div_height - 107) / 2;
        if (height < 780) {
            $("#account_top").css("padding-top", padding_top > 0 ? padding_top : 10);
        }
    });

    var getAgentInfo = (function () {
        var ret;
        $.ajax({
            url: "/home/getAgentInfo",
            type: "post",
            dataType: "json",
            success: function (rst) {
                ret = rst;
            },
            async: false
        });
        return ret;
    });
    var getHostName = (function () { return "@Html.Raw(CC.ERP.Portal.Global.Global_Domain)"; });
</script>
